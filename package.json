{
  "name": "encrypt-cli",
  "description": "Simple CLI for encrypting files using node.js Crypto library",
  "version": "v1.0.0",
  "author": "Chris Troutner @christroutner",
  "bin": {
    "encrypt-cli": "./bin/run"
  },
  "bugs": "https://github.com/christroutner/encrypt-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.10.4",
    "@oclif/plugin-help": "^2.1.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.2",
    "chai": "^4.2.0",
    "eslint": "^5.12.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/christroutner/encrypt-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "encrypt-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "christroutner/encrypt-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
